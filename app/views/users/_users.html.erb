<% if users.any? %>
  <ul class="media-list">
    <% users.each do |user| %>
      <li class="media">
        <div class="media-left">
          <img class="media-object img-rounded" src= " <%= "/images/#{user.image_name}" %>" alt="プロフィールイメージ">
        </div>
        <div class="media-body">
        <div>
          <div>
            <p><%= link_to user.name, user_path(user) %></p>
          </div>
        </div>
      </li>
    <% end %>
  </ul>

<% end %>